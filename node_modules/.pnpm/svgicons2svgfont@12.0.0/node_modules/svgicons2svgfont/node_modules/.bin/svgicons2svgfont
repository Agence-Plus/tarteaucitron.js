#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/bin/node_modules:/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/node_modules:/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules:/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/bin/node_modules:/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/node_modules:/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules:/home/lmoulanier/svn/web/trunk/public_html/js/tarteaucitron-1.19/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svgicons2svgfont.js" "$@"
else
  exec node  "$basedir/../../bin/svgicons2svgfont.js" "$@"
fi
